---
layout: layout.njk
permalink: "{{ pp.permalink | url }}/"

pagination:
  data: site.pages
  size: 1
  alias: pp

---

{% import "templates.njk" as template %}
{% switch pp.render %}

	{% case "--courses" %}
		{{ template.render_courses(pp) }}

	{% case "--course-individual" %}
		{{ template.render_course_individual(pp) }}

	{% default %}
		{{ pp | dump}}

{% endswitch %}

<style>pre{break-word: break-word;white-space: pre-wrap;}</style>
